{
	"name": "fpt-extension",
	"displayName": "fpt",
	"description": "FRC Programmer Trainer",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:fpt.extension.userFacing.getLevels",
		"onCommand:fpt.extension.userFacing.openLevel",
		"onCommand:fpt.extension.userFacing.getInstructions",
		"onCommand:fpt.extension.userFacing.verify"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "fpt.extension.userFacing.getLevels",
				"title": "FPT: Get Levels"
			},
			{
				"command": "fpt.extension.userFacing.openLevel",
				"title": "FPT: Open Level"
			},
			{
				"command": "fpt.extension.userFacing.getInstructions",
				"title": "FPT: Get Instructions"
			},
			{
				"command": "fpt.extension.userFacing.verify",
				"title": "FPT: Verify Level"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/random-js": "^1.0.31",
		"@types/randomstring": "^1.1.6",
		"marked-ts": "^1.0.0-beta.4",
		"random-js": "^2.1.0"
	}
}
